{
    "name": "mhd21/gateway",
    "homepage": "https://github.com/mhd21/gateway.git",
    "description": "forked from larabook/gateway to use in lumen",
    "minimum-stability": "dev",
    "require": {
        "php": "^5.3.3 || ^7.0",
        "mhd21/gateway": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Larabookir\\Gateway\\": "src/",
            "Larabookir\\Gateway\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larabookir\\Gateway\\GatewayServiceProvider"
            ],
            "aliases": {
                "Gateway": "Larabookir\\Gateway\\Gateway"
            }
        }
    }
}
